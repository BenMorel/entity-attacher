    "type": "library",
    "license": "LGPL-3",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
          "GeoSocio\\EntityAttacher\\": "src/"
        }
    },
    "require": {
